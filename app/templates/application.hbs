{{#if theme.isProvider}}
    {{title (concat theme.provider.name ' Registry') separator=' | '}}
    {{#if theme.stylesheet}}
        <style>
            @import url('{{theme.stylesheet}}');
        </style>
    {{/if}}
    {{preprint-navbar-branded model=theme.provider}}
{{else}}
    {{title 'OSF Registries' separator=' | '}}
    {{new-osf-navbar signupUrl=theme.signupUrl loginAction=(action 'login')}}
{{/if}}

{{maintenance-banner}}

{{outlet}}

{{#if (and notAuthenticated (not hasCookie))}}
    {{cookie-banner addCookie=(action 'addCookie')}}
{{/if}}

{{#if theme.isProvider}}
    {{preprint-footer-branded model=theme.provider}}
{{else}}
    {{osf-footer}}
    {{osf-copyright class='text-center'}}
    {{osf-mode-footer}}
{{/if}}
